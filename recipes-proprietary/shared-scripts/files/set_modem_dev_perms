#!/bin/sh

# Set device permissions
# Devices:
#    smem_log
#    diag
#    ttyHS0
#    ttyGS0
#    subsys_modem
#    dpl_ctrl
#    smdcntl8
#    smd7-x
# 
chown mdmuser:mdmuser /dev/smem_log
chown mdmuser:mdmuser /dev/diag
chown mdmuser:mdmuser /dev/ttyHS0
chown mdmuser:mdmuser /dev/ttyGS0
chown mdmuser:mdmuser /dev/subsys_modem
chown mdmuser:mdmuser /dev/dpl_ctrl
chown mdmuser:mdmuser /dev/smdcntl0
chown mdmuser:mdmuser /dev/smdcntl8
